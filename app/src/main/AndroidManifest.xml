<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="frsf.isi.dam.ejemplos01">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".GuardarEstadoActivity" />
        <activity android:name=".Actividad1Activity" />
        <activity android:name=".Actividad2Activity" />
        <activity android:name=".BloqueoMainThreadActivity" />

        <service
            android:name=".MyIntentService"
            android:exported="false"></service>
    </application>

</manifest>